{
  "name": "node-jwt-authentication-api",
  "version": "1.2.0",
  "description": "NodeJS JWT Authentication API",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/iArtz/node-jwt-authentication-api.git"
  },
  "scripts": {
    "start": "nodemon ./server.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|jsx)": [
      "eslint --fix"
    ],
    "*.+(json|css|md)": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "dotenv": "^8.2.0",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "jsonwebtoken": "^8.2.2",
    "rootpath": "^0.1.2"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-import-resolver-node": "^0.3.3",
    "eslint-plugin-import": "^2.20.1",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.7",
    "node-sass": "^4.13.1",
    "nodemon": "^1.17.5",
    "prettier": "^1.19.1"
  }
}
